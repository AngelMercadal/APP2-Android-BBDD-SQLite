<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/slytherinfBackground"
    tools:context=".Slytherin">

  <ImageView
      android:id="@+id/imageView4"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginStart="49dp"
      android:layout_marginTop="38dp"
      android:layout_marginEnd="56dp"
      android:layout_marginBottom="487dp"
      app:layout_constraintBottom_toTopOf="@+id/back_inicio4"
      app:layout_constraintEnd_toStartOf="@+id/back4"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/slytherin" />

  <Button
      android:id="@+id/back4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="104dp"
      android:layout_marginBottom="576dp"
      android:text="Button"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/imageView4"
      app:layout_constraintTop_toBottomOf="@+id/textView8" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="167dp"
      android:layout_height="0dp"
      android:layout_marginTop="38dp"
      android:layout_marginEnd="38dp"
      android:layout_marginBottom="18dp"
      android:text="TextView"
      app:layout_constraintBottom_toTopOf="@+id/back4"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <Button
      android:id="@+id/back_inicio4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="53dp"
      android:text="Button"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView4" />
</androidx.constraintlayout.widget.ConstraintLayout>